elastix:
    image: willdurand/elk
    ports:
        - "80:80"
        - "5043:5043"
    volumes_from:
        - data

data:
    image: busybox
    volumes:
        - @ELASTIX_DASHBOARD_SOURCE_DIR@:/etc/ssl
        - @ELASTIX_DASHBOARD_VOLUME@:/data                   
        - @ELASTIX_DASHBOARD_BINARY_DIR@/Logstash:/etc/logstash
