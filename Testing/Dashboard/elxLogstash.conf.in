input {
  lumberjack {
    port => @ELASTIX_DASHBOARD_LOGSTASH_PORT@
    ssl_certificate => "/etc/ssl/@ELASTIX_DASHBOARD_LOGSTASH_CERTIFICATE@"
    ssl_key => "/etc/ssl/@ELASTIX_DASHBOARD_LOGSTASH_RSA_PRIVATE_KEY@"
  }
}

filter {
  xml {
    source => message
    target => result
  }
}

output {
  elasticsearch {
  	protocol => http
  }
}