{
  "network": {
    "servers": [ "elastix:5043" ],
    "timeout": 15,
    "ssl certificate": "/etc/ssl/@ELASTIX_DASHBOARD_LOGSTASH_CERTIFICATE@",
    "ssl key": "/etc/ssl/@ELASTIX_DASHBOARD_LOGSTASH_RSA_PRIVATE_KEY@",
    "ssl ca": "/etc/ssl/@ELASTIX_DASHBOARD_LOGSTASH_CERTIFICATE@"
  },
  "files": [
    {
      "paths": [
        "/var/unit/*.xml"
       ],
      "fields": { 
        "type": "unittest",
        "lib": "elastix" 
      }
    },
    {
      "paths": [
        "/var/benchmark/*.xml"
       ],
      "fields": { 
        "type": "benchmark",
        "lib": "elastix" 
      }
    }
   ]
}
